Object subclass: #NodeParent	instanceVariableNames: 'tagName'	classVariableNames: ''	poolDictionaries: ''	category: 'Tp3 - HTML'!!NodeParent methodsFor: 'as yet unclassified' stamp: 'matiasgarcia 11/9/2015 18:54'!setTagName: name	"Guarda el nombre de la etiqueta"	tagName := name.! !!NodeParent methodsFor: 'as yet unclassified' stamp: 'matiasgarcia 11/9/2015 23:32'!openTag: ws	"comment stating purpose of message"	ws << '<',tagName,'>'.	^ws! !!NodeParent methodsFor: 'as yet unclassified' stamp: 'matiasgarcia 11/9/2015 23:32'!closeTag: ws	"comment stating purpose of message"	ws << '</',tagName,'>'.	^ws! !NodeParent subclass: #HtmlNode	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Tp3 - HTML'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!HtmlNode class	instanceVariableNames: ''!!HtmlNode class methodsFor: 'as yet unclassified' stamp: 'matiasgarcia 11/9/2015 23:16'!new	"Crea una nueva etiqueta"	^(super new) setTagName: 'html'! !TestCase subclass: #TestsHtml	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Tp3 - HTML-Tests'!!TestsHtml commentStamp: '<historical>' prior: 0!A TestsTest is a test class for testing the behavior of Tests!!TestsHtml methodsFor: 'tests-as yet unclassified' stamp: 'matiasgarcia 11/9/2015 18:59'!testTesteEjercicio01CreaTagHtml|ws node1 |ws := WriteStream on: String new. node1 := HtmlNode new.node1 openTag: ws.node1 closeTag: ws.self assert: (ws contents) equals: '<html></html>'! !